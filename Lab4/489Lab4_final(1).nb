Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"X", "[", "k", "]"}], " ", "=", " ", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"n", "=", "0"}], 
      RowBox[{"N", "-", "1"}]], 
     RowBox[{
      RowBox[{"x", "[", "n", "]"}], " ", 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{
        RowBox[{"-", " ", "j"}], " ", 
        FractionBox[
         RowBox[{"2", "\[Pi]"}], "N"], "k", " ", "n"}]]}]}]}], 
   TraditionalForm]],ExpressionUUID->"834cf423-6da1-4a1b-bfab-1a156111527a"],
 ",\t\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"k", "=", "0"}], ",", "1", ",", "...", ",", 
    RowBox[{"N", "-", "1"}]}], TraditionalForm]],ExpressionUUID->
