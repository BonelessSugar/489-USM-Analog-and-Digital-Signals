Cell["The inverse discrete Fourier transform (IDFT) is given by", "Text",ExpressionUUID->"b1d20838-c012-47cf-a2c8-9d7b9204dc7d"],

Cell[TextData[{
 "\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"x", "[", "n", "]"}], " ", "=", 
    RowBox[{
     FractionBox["1", "N"], " ", 
     RowBox[{
      UnderoverscriptBox["\[Sum]", 
       RowBox[{"k", "=", "0"}], 
       RowBox[{"N", "-", "1"}]], 
      RowBox[{
       RowBox[{"X", "[", "k", "]"}], "  ", 
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{" ", 
         RowBox[{"j", " ", 
          FractionBox[
           RowBox[{"2", "\[Pi]"}], "N"], "k", " ", "n"}]}]]}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"89dfadf2-a51d-4efa-9274-92b38dfa6563"],
 ",\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"n", "=", "0"}], ",", "1", ",", "...", ",", 
    RowBox[{"N", "-", "1"}]}], TraditionalForm]],ExpressionUUID->
